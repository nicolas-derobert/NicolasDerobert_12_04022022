
{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"],
      "allowUnknownTags": ["component"] //or true
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "better-docs/component","node_modules/better-docs/component","node_modules/better-docs/typescript"],
    
    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }