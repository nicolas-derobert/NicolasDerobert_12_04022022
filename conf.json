
{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"],
      "allowUnknownTags": ["component"] 
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "better-docs/component","node_modules/better-docs/typescript","node_modules/better-docs/category"],
    "templates": {
      "better-docs": {
        "name": "Sportsee",
        "logo": "../src/assets/images/logo.svg",
        "title": "Sportsee Project Documentaion"
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md",
      "template": "node_modules/better-docs"
    }
  }